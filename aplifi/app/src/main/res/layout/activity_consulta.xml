<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".consulta">
    <TableLayout android:layout_width="335dp" android:layout_height="668dp"
                 app:layout_constraintTop_toTopOf="parent"
                 app:layout_constraintEnd_toEndOf="parent" app:layout_constraintBottom_toBottomOf="parent"
                 app:layout_constraintStart_toStartOf="parent">
        <TableRow android:layout_width="225dp" android:layout_height="80dp">
            <TextView android:layout_height="40dp" android:layout_width="220dp" android:layout_marginLeft="50dp"
            android:id="@+id/te1"/>
        </TableRow>
        <TableRow android:layout_width="225dp" android:layout_height="80dp">
            <TextView android:layout_height="40dp" android:layout_width="220dp" android:layout_marginLeft="50dp"
                      android:id="@+id/te2"/>
        </TableRow>
        <TableRow android:layout_width="225dp" android:layout_height="80dp">
            <TextView android:layout_height="40dp" android:layout_width="220dp" android:layout_marginLeft="50dp"
                      android:id="@+id/te3"/>
        </TableRow>
        <TableRow android:layout_width="225dp" android:layout_height="80dp">
            <TextView android:layout_height="40dp" android:layout_width="220dp" android:layout_marginLeft="50dp"
                      android:id="@+id/te4"/>
        </TableRow>
        <TableRow android:layout_width="225dp" android:layout_height="80dp">
            <TextView android:layout_height="40dp" android:layout_width="220dp" android:layout_marginLeft="50dp"
                      android:id="@+id/te5"/>
        </TableRow>
        <TableRow android:layout_width="225dp" android:layout_height="80dp">
            <ImageView
                    android:layout_width="206dp"
                    android:layout_height="254dp" app:srcCompat="?attr/colorBackgroundFloating"
                    android:id="@+id/te6"
                    tools:srcCompat="@android:color/black" android:layout_marginTop="8dp"
                    android:layout_marginLeft="80dp"

                    android:layout_marginBottom="15dp"/>
        </TableRow>

    </TableLayout>


</android.support.constraint.ConstraintLayout>